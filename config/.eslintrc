{
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest",
    "extraFileExtensions": [".vue"]
  },
  "extends": ["plugin:vue/vue3-essential"],
  "rules": {
    "vue/multi-word-component-names": "off",
    "no-unused-vars": "warn",
    "semi": ["warn", "always"],
    "quotes": ["warn", "single"]
  }
}
